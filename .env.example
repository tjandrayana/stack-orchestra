# Stack Orchestra Configuration
# Copy this file to .env and customize as needed
# cp .env.example .env

# Project Configuration
COMPOSE_PROJECT_NAME=stack-orchestra
NETWORK_NAME=stack-orchestra
CONTAINER_PREFIX=stack-orchestra

# PostgreSQL Configuration
POSTGRES_IMAGE=postgres:16
POSTGRES_DB=stack_orchestra
POSTGRES_USER=orchestra
POSTGRES_PASSWORD=orchestra
POSTGRES_PORT=5432

# MySQL Configuration
MYSQL_IMAGE=mysql:8.0
MYSQL_DATABASE=stack_orchestra
MYSQL_USER=orchestra
MYSQL_PASSWORD=orchestra
MYSQL_ROOT_PASSWORD=mysqlroot
MYSQL_PORT=3306

# MongoDB Configuration
MONGODB_IMAGE=mongo:7.0
MONGODB_ROOT_USERNAME=orchestra
MONGODB_ROOT_PASSWORD=orchestra
MONGODB_PORT=27017

# Neo4j Configuration
NEO4J_IMAGE=neo4j:5.23
NEO4J_AUTH=neo4j/stackorchestra
NEO4J_PASSWORD=stackorchestra
NEO4J_HTTP_PORT=7474
NEO4J_BOLT_PORT=7687

# ArangoDB Configuration
ARANGODB_IMAGE=arangodb:3.12
ARANGODB_ROOT_PASSWORD=stackorchestra
ARANGODB_PORT=8529

# Redis Configuration
REDIS_IMAGE=redis:7.4-alpine
REDIS_PORT=6379

# Elasticsearch Configuration
ELASTICSEARCH_IMAGE=docker.elastic.co/elasticsearch/elasticsearch:7.17.22
ELASTICSEARCH_HTTP_PORT=9200
ELASTICSEARCH_TRANSPORT_PORT=9300
ELASTICSEARCH_DISCOVERY_TYPE=single-node
ELASTICSEARCH_SECURITY_ENABLED=false
ELASTICSEARCH_JAVA_OPTS=-Xms1g -Xmx1g

# Kibana Configuration
KIBANA_IMAGE=docker.elastic.co/kibana/kibana:7.17.22
KIBANA_PORT=5601
KIBANA_ELASTICSEARCH_HOSTS=http://elasticsearch:9200

# Nginx Configuration
NGINX_IMAGE=nginx:1.27-alpine
NGINX_PORT=8080

# OpenResty Configuration
OPENRESTY_IMAGE=openresty/openresty:1.25.3.1-2-alpine-fat
OPENRESTY_PORT=8081

# Prometheus Configuration
PROMETHEUS_IMAGE=prom/prometheus:latest
PROMETHEUS_PORT=9090

# Grafana Configuration
GRAFANA_IMAGE=grafana/grafana:latest
GRAFANA_PORT=3000
GRAFANA_ADMIN_USER=admin
GRAFANA_ADMIN_PASSWORD=admin
GRAFANA_INSTALL_PLUGINS=

# ScyllaDB Configuration
SCYLLA_IMAGE=scylladb/scylla:latest
SCYLLA_CQL_PORT=9042
SCYLLA_REST_PORT=10000
SCYLLA_INTER_NODE_PORT=7000
SCYLLA_INTER_NODE_TLS_PORT=7001

# Qdrant Configuration
QDRANT_IMAGE=qdrant/qdrant:v1.7.4
QDRANT_HTTP_PORT=6333
QDRANT_GRPC_PORT=6334

# Milvus Configuration
MILVUS_IMAGE=milvusdb/milvus:v2.3.4
MILVUS_GRPC_PORT=19530
MILVUS_METRICS_PORT=9091

# Milvus Dependencies
ETCD_IMAGE=quay.io/coreos/etcd:v3.5.5
MINIO_IMAGE=minio/minio:RELEASE.2023-03-20T20-16-18Z
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin
